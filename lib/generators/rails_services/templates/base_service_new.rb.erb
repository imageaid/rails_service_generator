<%= '# Generated service object' %>
<%= "module #{module_name}" %>
<% if subfolder_name.present? -%>
  <%= "module #{subfolder_name}" %>
    <%= '##' %>
    <%= "# #{qualified_name} Service Class"  %>
    <%= "class #{service_class_name}" %>
      <%= 'include BaseService' %>

      <%= "# SAMPLE USAGE: #{qualified_name}.new(*args).call" %>
      <%= 'def initialize(_args);' %> <%= 'end' %>

      <%= 'def call;' %> <%= 'end' %>
    <%= 'end' %>
  <%= 'end' %>
<% else -%>
  <%= '##' %>
  <%= "# #{qualified_name} Service Class"  %>
  <%= "class #{service_class_name}" %>
    <%= 'include BaseService' %>

    <%= "# SAMPLE USAGE: #{qualified_name}.new(*args).call" %>
    <%= 'def initialize(_args);' %> <%= 'end' %>

    <%= 'def call;' %> <%= 'end' %>
  <%= 'end' %>
<% end -%>
<%= 'end' %>
